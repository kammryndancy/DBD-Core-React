32400 ! ^32400,10
32410 CALC_VEND_TOTALS:
32415 SELL_BREAK_IN_EACHES=VIN(_ROWN1$) ! FN%QTY_IN_EACHES(NUM(EVS(_ROWN1$)),COST_UM$,NUM(COST_QTY_PER$))!SSP#228592

10020 SHIP_QTY_PER_UNIT$=STR(SHIP_QTY_PER_UNIT)
10030 SHIP_QTY$=STR(SHIP_QTY)
10040 OF3_UNUSED_2$=STR(OF3_UNUSED_2)

14000 ! 14000 - Enable/Disable groups - _ENABLE_FLG: 0=don't!, >1=corresponding key segment on, others off, -1=buttons and non-key fields on
14010 ENABLE_GROUPS:
14020 IF _ENABLE_FLG=0 THEN GOTO *RETURN
14030 IF _ENABLE_FLG=2 THEN CALL "*wingrp;Enable",KEY2.GRP$; NEXT_ID=PO_CODE.CTL ELSE CALL "*wingrp;Disable",KEY2.GRP$
14040 IF _ENABLE_FLG=3 THEN CALL "*wingrp;Enable",KEY3.GRP$; NEXT_ID=GOE_LINE_NUMBER.CTL ELSE CALL "*wingrp;Disable",KEY3.GRP$
14050 IF _ENABLE_FLG=4 THEN CALL "*wingrp;Enable",KEY4.GRP$; NEXT_ID=GOE_SEQ_NUMBER.CTL ELSE CALL "*wingrp;Disable",KEY4.GRP$
14970 IF _ENABLE_FLG>0 THEN CALL "*wingrp;Disable",FIELDS.GRP$; CALL "*wingrp;Disable",BUTTONS.GRP$
14980 IF _ENABLE_FLG<0 THEN CALL "*wingrp;Enable",FIELDS.GRP$; CALL "*wingrp;Enable",BUTTONS.GRP$
14990 RETURN 
15000 ! 15000 - Determine the changes that have been made before writing
15010 DETERMINE_CHGS:
15020 DIM _CHG_VAR$(32,"N")
15030 IF _ORIG.GOE_NUMBER$=_CUR.GOE_NUMBER$ OR GOE_NUMBER$=_CUR.GOE_NUMBER$ THEN GOTO *NEXT ELSE IF _ORIG.GOE_NUMBER$=GOE_NUMBER$ THEN _CHG2$=_CHG2$+SEP+MSG("GOE_NUMBER")+" = "+_CUR.GOE_NUMBER$,_CHG_VAR$(1,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("GOE_NUMBER")+" = "+_CUR.GOE_NUMBER$
15040 IF _ORIG.PO_CODE$=_CUR.PO_CODE$ OR PO_CODE$=_CUR.PO_CODE$ THEN GOTO *NEXT ELSE IF _ORIG.PO_CODE$=PO_CODE$ THEN _CHG2$=_CHG2$+SEP+MSG("PO_CODE")+" = "+_CUR.PO_CODE$,_CHG_VAR$(2,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("PO_CODE")+" = "+_CUR.PO_CODE$
15050 IF _ORIG.GOE_LINE_NUMBER$=_CUR.GOE_LINE_NUMBER$ OR GOE_LINE_NUMBER$=_CUR.GOE_LINE_NUMBER$ THEN GOTO *NEXT ELSE IF _ORIG.GOE_LINE_NUMBER$=GOE_LINE_NUMBER$ THEN _CHG2$=_CHG2$+SEP+MSG("GOE_L_N")+" = "+_CUR.GOE_LINE_NUMBER$,_CHG_VAR$(3,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("GOE_L_N")+" = "+_CUR.GOE_LINE_NUMBER$
15060 IF _ORIG.GOE_SEQ_NUMBER$=_CUR.GOE_SEQ_NUMBER$ OR GOE_SEQ_NUMBER$=_CUR.GOE_SEQ_NUMBER$ THEN GOTO *NEXT ELSE IF _ORIG.GOE_SEQ_NUMBER$=GOE_SEQ_NUMBER$ THEN _CHG2$=_CHG2$+SEP+MSG("GOE_S_N")+" = "+_CUR.GOE_SEQ_NUMBER$,_CHG_VAR$(4,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("GOE_S_N")+" = "+_CUR.GOE_SEQ_NUMBER$
15070 IF _ORIG.LOCATION_TYPE$=_CUR.LOCATION_TYPE$ OR LOCATION_TYPE$=_CUR.LOCATION_TYPE$ THEN GOTO *NEXT ELSE IF _ORIG.LOCATION_TYPE$=LOCATION_TYPE$ THEN _CHG2$=_CHG2$+SEP+MSG("LOCA_T")+" = "+_CUR.LOCATION_TYPE$,_CHG_VAR$(5,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("LOCA_T")+" = "+_CUR.LOCATION_TYPE$
15080 IF _ORIG.SHIP_TO_CODE$=_CUR.SHIP_TO_CODE$ OR SHIP_TO_CODE$=_CUR.SHIP_TO_CODE$ THEN GOTO *NEXT ELSE IF _ORIG.SHIP_TO_CODE$=SHIP_TO_CODE$ THEN _CHG2$=_CHG2$+SEP+MSG("SHIP_T_C")+" = "+_CUR.SHIP_TO_CODE$,_CHG_VAR$(6,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("SHIP_T_C")+" = "+_CUR.SHIP_TO_CODE$
15090 IF _ORIG.LOCATION_CODE$=_CUR.LOCATION_CODE$ OR LOCATION_CODE$=_CUR.LOCATION_CODE$ THEN GOTO *NEXT ELSE IF _ORIG.LOCATION_CODE$=LOCATION_CODE$ THEN _CHG2$=_CHG2$+SEP+MSG("LOC_CODE")+" = "+_CUR.LOCATION_CODE$,_CHG_VAR$(7,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("LOC_CODE")+" = "+_CUR.LOCATION_CODE$
15100 IF _ORIG.REC_DEPT$=_CUR.REC_DEPT$ OR REC_DEPT$=_CUR.REC_DEPT$ THEN GOTO *NEXT ELSE IF _ORIG.REC_DEPT$=REC_DEPT$ THEN _CHG2$=_CHG2$+SEP+MSG("REC_DEPT")+" = "+_CUR.REC_DEPT$,_CHG_VAR$(8,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("REC_DEPT")+" = "+_CUR.REC_DEPT$
15110 IF _ORIG.ATTN_LINE$=_CUR.ATTN_LINE$ OR ATTN_LINE$=_CUR.ATTN_LINE$ THEN GOTO *NEXT ELSE IF _ORIG.ATTN_LINE$=ATTN_LINE$ THEN _CHG2$=_CHG2$+SEP+MSG("ATTN_LINE")+" = "+_CUR.ATTN_LINE$,_CHG_VAR$(9,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("ATTN_LINE")+" = "+_CUR.ATTN_LINE$
15120 IF _ORIG.SHIP_ADDR1$=_CUR.SHIP_ADDR1$ OR SHIP_ADDR1$=_CUR.SHIP_ADDR1$ THEN GOTO *NEXT ELSE IF _ORIG.SHIP_ADDR1$=SHIP_ADDR1$ THEN _CHG2$=_CHG2$+SEP+MSG("SHIP_ADDR1")+" = "+_CUR.SHIP_ADDR1$,_CHG_VAR$(10,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("SHIP_ADDR1")+" = "+_CUR.SHIP_ADDR1$
15130 IF _ORIG.SHIP_ADDR2$=_CUR.SHIP_ADDR2$ OR SHIP_ADDR2$=_CUR.SHIP_ADDR2$ THEN GOTO *NEXT ELSE IF _ORIG.SHIP_ADDR2$=SHIP_ADDR2$ THEN _CHG2$=_CHG2$+SEP+MSG("SHIP_ADDR2")+" = "+_CUR.SHIP_ADDR2$,_CHG_VAR$(11,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("SHIP_ADDR2")+" = "+_CUR.SHIP_ADDR2$
15140 IF _ORIG.SHIP_CITY$=_CUR.SHIP_CITY$ OR SHIP_CITY$=_CUR.SHIP_CITY$ THEN GOTO *NEXT ELSE IF _ORIG.SHIP_CITY$=SHIP_CITY$ THEN _CHG2$=_CHG2$+SEP+MSG("SHIP_CITY")+" = "+_CUR.SHIP_CITY$,_CHG_VAR$(12,1)="Y" ELSE _CHG1$=_CHG1$+SEP+MSG("SHIP_CITY")+" = "+_CUR.SHIP_CITY$

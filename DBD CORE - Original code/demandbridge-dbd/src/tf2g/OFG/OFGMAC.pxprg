13030 IF _CLR_FLG$="R" THEN PO_CODE$=""
13040 IF _CLR_FLG$="R" THEN GOE_LINE_NUMBER$=""
13050 IF _CLR_FLG$="R" THEN GOE_SEQ_NUMBER$=""

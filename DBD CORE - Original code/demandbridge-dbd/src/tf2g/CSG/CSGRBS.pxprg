25920 IF _YESNO$="NO" THEN GOSUB MAIN_POST_DISPLAY; EXIT 
25930 DIM A$(73),R0$(128),W$(53),RNG(4)
25940 A$(71,1)="1",A$(1,6)="CS2RBA",A$(7,LEN(RPT_SELEC$))=RPT_SELEC$
25950 WRITE (_FIL_NO,KEY=_KEY$)A$,R0$,W$,RNG{ALL}

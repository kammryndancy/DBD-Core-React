0567 LIST_BOX WRITE LB_CODES.CTL,ML_AUDIT_NO$
0570 B_START_FISCAL$=START_FISCAL$,B_START_ACCTPD$=START_ACCTPD$; ONLY_OPEN$="1"; PERFORM "GLGMDA;LOAD_FISCAL_YEAR"

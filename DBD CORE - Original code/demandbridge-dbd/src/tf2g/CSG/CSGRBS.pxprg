13000 ! 13000 - Clear the record fields
14000 ! 14000 - Enable/Disable groups - _ENABLE_FLG: 0=don't!, >1=corresponding key segment on, others off, -1=buttons and non-key fields on
14010 ENABLE_GROUPS:
14020 IF _ENABLE_FLG=0 THEN GOTO *RETURN
14970 IF _ENABLE_FLG>0 THEN CALL "*wingrp;Disable",FIELDS.GRP$; CALL "*wingrp;Disable",BUTTONS.GRP$
14980 IF _ENABLE_FLG<0 THEN CALL "*wingrp;Enable",FIELDS.GRP$; CALL "*wingrp;Enable",BUTTONS.GRP$
14990 RETURN 
15000 ! 15000 - Determine the changes that have been made before writing
16000 ! 16000 - Incorporate changes made by another user into the record
20000 VIEW_REPORT:
20001 PRINT_REPORT:
20010 ! View Report Routine.
20020 IOLIST A$
20030 IOLIST R0$,W$
20040 ZZPARM=HFN; OPEN (ZZPARM)"ZZPARM"; READ (ZZPARM,KEY=FID(0)+"      ")X3$,*,X4$; READ (ZZPARM,KEY="STATI/C")T1$,F0
20050 ZZP=HFN; OPEN (ZZP)"ZZP"
20060 ! 
20070 GOSUB SEL_DATE; IF CHANGE_FLG THEN GOSUB CHECK_CHANGES ELSE GOSUB ASSIGN_PRG
20080 A$(1,6)="CS2RBA"
20090 IF ID$="RPT_VEIW" THEN R0$(2,6)="P_    "
20100 ! 
20260 K$=X3$(1,8),%GUI=1,T1$="CS5   "
20267 %B_WINDATE_FMT$=%WINDATE_FMT$,%WINDATE_FMT$="YYYYMMDD",JJ$=DTE(0:"%Yl%Mz%Dz"); CALL "ZZWDTE;VALIDATE",JJ$,JJJ$,JJJJ$; X3$(21,6)=JJ$
20270 WRITE (ZZP,KEY=K$)X3$,X4$,T1$,R0$,W$,A$
20275 WIN_D$=FIN(0),WIN_CURR=DEC(WIN_D$(10,1))
20276 PRINT 'CURSOR'(1),
20280 K9$="CS2RBA" ! SSP 229934
20281 IF RB_PRT$>"0" THEN CALL "ZZ2DRV",K9$ ELSE CALL K9$ ! SSP 229934
20282 PRINT 'CURSOR'(0),
20285 CMD_STR$="END"
20290 EXIT 
25010 ASSIGN_LIB:
25020 ! Output Option
25030 IF NUM(A$(71,1))=0 THEN A$(71,1)="1"
25040 PRT_MODE$=R0$(1,1),RPT_OUTDEV$=R0$(2,2)
25050 RPT_VERT$=R0$(9,1),RPT_HORIZ$=R0$(10,1)
25060 RPT_AUX1$=R0$(11,1),RPT_AUX2$=R0$(12,1)
25070 RPT_LINES$=R0$(15,2),RPT_FORMCODE$=R0$(17,1)
25080 RPT_JOBCARD$=MID(R0$,65,60),RPT_JOBCARD_FLG$=MID(R0$,125,1)
25090 RPT_AUX3$=R0$(13,1),RPT_AUX4$=R0$(14,1)
25100 ! Selection Options
25110 RPT_SELEC$=A$(7,12),RPT_DESC$=A$(19,40),RPT_OPT$=A$(59,12),RPT_SEQ$=A$(71,1)
25130 IF STP(RPT_OPT$)="" THEN RPT_OPT$=""
25140 IF STP(RPT_SELEC$)="" THEN RPT_SELEC$=""
25150 REFRESH_FLG=1; DIM S$(10,"~")
25160 SWITCH RPT_SEQ$
25170 CASE "1"
25372 CUST_START$=W$(1,10); IF W$(11,10)<>S$ THEN CUST_END$=W$(11,10) ! SSP#229934
25380 ITEM_START$=STP(W$(21,10)); IF W$(31,10)<>S$ THEN ITEM_END$=W$(31,10)
25410 BREAK
25520 END SWITCH 
25530 RETURN 
25540 ASSIGN_PRG:
25550 A$(1,6)=ARG_1$,A$(7,12)=RPT_SELEC$,A$(19,40)=RPT_DESC$,A$(59,12)=RPT_OPT$,A$(71,1)=RPT_SEQ$
25560 A$(59,12)=RPT_OPT$; A$(71,1)=RPT_SEQ$
25570 ! 
25580 R0$(1,1)=PRT_MODE$,R0$(2,6)=MID(RPT_OUTDEV$,1,2)+"    ",R0$(9,1)=RPT_VERT$,R0$(10,1)=RPT_HORIZ$
25590 R0$(11,1)=RPT_AUX1$,R0$(12,1)=RPT_AUX2$,R0$(13,1)=RPT_AUX3$,R0$(14,1)=RPT_AUX4$
25600 R0$(15,2)=RPT_LINES$,R0$(17,1)=RPT_FORMCODE$,R0$(65,60)=RPT_JOBCARD$
25620 R0$(125,1)=RPT_JOBCARD_FLG$; DIM W$(51,"~")
25630 SWITCH RPT_SEQ$
25650 CASE "1"
25655 DIM W$(53,"~")
25661 W$(1,10)=CUST_START$; IF STP(CUST_END$)<>"" THEN W$(11,10)=CUST_END$
25664 W$(21,10)=ITEM_START$; IF STP(ITEM_END$)<>"" THEN W$(31,10)=ITEM_END$
25720 BREAK
25750 END SWITCH 
25760 RETURN 
25770 SEL_DATE:
25780 IF LEN(START_FISCAL$)>4 OR STP(START_FISCAL$)="" THEN TF_YEAR$=%CR_YEAR$ ELSE TF_YEAR$=START_FISCAL$
25790 IF LEN(START_ACCTPD$)>2 OR STP(START_ACCTPD$)="" THEN PER$=%CR_PD$ ELSE PER$=START_ACCTPD$
25800 FIND (_ZZPARM,KEY=%C$+"G/LYE"+TF_YEAR$)REC_DAT$; REC_DAT$=REC_DAT$(21)
25810 DIM CUR_DATE$[12]
25820 FOR XX=1 TO 12; CUR_DATE$[XX]=FND$(REC_DAT$(XX*6-5,6)); NEXT XX
25830 ML_ENDING_PERIOD$=CUR_DATE$[NUM(PER$)]
25840 REFRESH_FLG=1

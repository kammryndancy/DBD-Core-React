0565 EXECUTE "next_folder=FLDR."+_F_OBJ$[NUM(A$(71,1))]+".CTL,REFRESH_FLG=1"
0566 NEXT_ID=RPT_VEIW.CTL,REFRESH_FLG=1,RPT_SEQ=NUM(A$(71,1))
0567 GOSUB ASSIGN_LIB
0570 RETURN 
0900 ! 900 - Wrapup
0910 WRAPUP:
0920 ARG_1$=KEC(_FIL_NO,ERR=*NEXT)

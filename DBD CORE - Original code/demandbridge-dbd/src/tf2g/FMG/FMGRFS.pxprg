0050 PROCESS "FMGRFS","../FMG/FM.EN"
0060 EXIT 
0090 ! 
0100 ! 100 - Initialization
0110 INIT:
0120 _SV_KR=PRM('KR'); SET_PARAM 'KR'=0 ! Ensure native handling of KEP()
0130 X0=-1,X2=-1
0140 CALL "ZZCOMP",X0$,X1$,X2$,X3$,X4$,M9$,X0,X1,X2
0150 ! 
0155 DIM ZZS$(112),RNG$(128),PRT$(128),RNG[4],B.RNG[4]
0160 ! iolist
0170 IOLIST ZZS$
0180 IOLIST PRT$,RNG$,RNG{ALL}
0300 ! Open Files
0310 DIM Z[NUM(X3$(60,3))]
0320 Z$="01O ZZS...  06O ZZPARM  07O ZZP     "
0330 CALL "ZZFLES",X3$,Y1$,Y0$,Z$,Z{ALL},Z0,Z1
0400 ! 
0410 RPT_SELEC$="            "; VIEW$=""
0490 RETURN 
0500 ! 500 - Main panel post_display logic
0510 MAIN_POST_DISPLAY:
0520 KEY_1$="FM2RFA"+RPT_SELEC$,KEY_1$=PAD(KEY_1$,18," "); %K9$="FM2RFA"
0530 READ (Z[1],KEY=KEY_1$,DOM=ADD_OPT)IOL=0170,IOL=0180
0535 B_ZZS$=ZZS$; B_PRT$=PRT$; B_RNG$=RNG$; CHG_FLG=0
0536 NEXT_FOLDER=11000+NUM(ZZS$(71,1)),NEXTFOLDER_FLG=NEXT_FOLDER,REFRESH_FLG=1
0580 GOSUB SELECT_OPTIONS
0590 RETURN 
0600 ! SELECTION OPTIONS
0610 SELECT_OPTIONS:
0620 PRG_NAME$=ZZS$(1,6); RPT_SELEC$=ZZS$(7,12); RPT_DESC$=ZZS$(19,40)
0630 RPT_OPT$=ZZS$(59,12); RPT_SEQ$=ZZS$(71,1)
0635 ! 
0640 DB_NOTICE$=ZZS$(72,1),TF_DATE$=ZZS$(73,6),CB_1$=ZZS$(79,1),CB_2$=ZZS$(111,1),CB_3$=ZZS$(112,1)
0650 IF NUL(CB_1$) THEN CB_1$="N"
0660 IF NUL(CB_2$) THEN CB_2$="N"
0670 IF NUL(CB_3$) THEN CB_3$="N"
0800 ! Output Options
0810 PRT_MODE$=PRT$(1,1); RPT_OUTDEV$=PRT$(2,6)
0820 RPT_VERT$=PRT$(9,1); RPT_HORIZ$=PRT$(10,1)
0830 RPT_AUX1$=PRT$(11,1); RPT_AUX2$=PRT$(12,1); RPT_AUX3$=PRT$(13,1); RPT_AUX4$=PRT$(14,1)
0840 RPT_LINES$=PRT$(15,2); RPT_FORMCODE$=PRT$(17,1)
0850 RPT_JOBCARD$=PRT$(65,60); RPT_JOBCARD_FLG$=PRT$(125,1)
0860 GOSUB LOAD_RANGE
0870 NEXT_ID=RPT_VEIW.CTL
0880 REFRESH_FLG=1
0890 RETURN 
0900 ! 900 - Wrapup
0910 WRAPUP:
0920 CALL "ZZFLES",X3$,Y1$,Y0$,"END",Z{ALL},0,0
0960 SET_PARAM 'KR'=_SV_KR
0970 EXIT 
1000 ! load the sequence range
1010 LOAD_RANGE:
1020 SWITCH RPT_SEQ$
1030 CASE "1"
1041 IF NUL(RNG$(1,1)) THEN NOT_TYPE_START$="" ELSE NOT_TYPE_START$=RNG$(1,1)
1042 IF RNG$(2,1)="~" THEN NOT_TYPE_END$="" ELSE NOT_TYPE_END$=RNG$(2,1)
1043 IF NUL(RNG$(3,1)) THEN NOT_FORMAT_START$="" ELSE NOT_FORMAT_START$=RNG$(3,1)
1044 IF RNG$(4,1)="~" THEN NOT_FORMAT_END$="" ELSE NOT_FORMAT_END$=RNG$(4,1)
1045 IF NUL(RNG$(5,10)) THEN CUST_START$="" ELSE CUST_START$=RNG$(5,10)
1046 IF RNG$(15,3)="~~~" THEN CUST_END$="" ELSE CUST_END$=RNG$(15,10)
1047 IF NUL(RNG$(25,4)) THEN LOC_START$="" ELSE LOC_START$=RNG$(25,4)
1048 IF RNG$(29,4)="~~~~" THEN LOC_END$="" ELSE LOC_END$=RNG$(29,4)
1049 IF NUL(RNG$(33,10)) THEN ITEM_START$="" ELSE ITEM_START$=RNG$(33,10)
1050 IF RNG$(43,10)="~~~~~~~~~~" THEN ITEM_END$="" ELSE ITEM_END$=RNG$(43,10)
1085 NEXTFOLDER_FLG=FLDR.FM1.16.CTL
1090 RETURN 
1490 END SWITCH 
1500 RETURN 
3800 CHECK_CHANGES:
3810 GOSUB LOAD_OPTIONS
3820 IF CHG_FLG=0 THEN RETURN 
3830 MSGBOX MSG("UPD_RPT_1")+SEP+MSG("UPD_RPT_2"),MSG("FYI"),"?,YesNo",ANS$
3840 IF ANS$="NO" THEN RETURN 
3850 WRITE (Z[1],KEY=KEY_1$)IOL=0170,IOL=0180
3860 B_ZZS$=ZZS$; B_PRT$=PRT$; B_RNG$=RNG$; CHG_FLG=0
3870 RETURN 
8000 ! Messages
8010 ADD_OPT:
8020 MSGBOX MSG("ADD_RECORD"),MSG("CONFIRM"),"?,YesNo",ANS$
8030 IF ANS$="NO" THEN CMD_STR$="END"; RETURN 
8040 KEY_1$="FM2RFA"+RPT_SELEC$
8050 DIM ZZS$(112),RNG$(128),PRT$(128); ZZS$(1,18)=KEY_1$,ZZS$(71,1)="1"
8060 GOSUB SELECT_OPTIONS
8070 REFRESH_FLG=1; NEXT_ID=RPT_DESC.CTL
8090 RETURN 
9999 END 
17000 ! 
20000 LOAD_OPTIONS:
20010 ! View Report Routine.
20020 ! 
20030 ZZS$(1,6)=PRG_NAME$
20040 ZZS$(7,12)=RPT_SELEC$; ZZS$(19,40)=RPT_DESC$
20050 ZZS$(59,12)=RPT_OPT$; ZZS$(71,1)=RPT_SEQ$
20055 ! 
20060 ZZS$(72,1)=DB_NOTICE$,ZZS$(73,6)=TF_DATE$,ZZS$(79,1)=CB_1$,ZZS$(111,1)=CB_2$,ZZS$(112,1)=CB_3$
20100 ! 
20110 PRT$(1,1)=PRT_MODE$; PRT$(2,2)=RPT_OUTDEV$
20120 PRT$(9,1)=RPT_VERT$; PRT$(10,1)=RPT_HORIZ$
20130 PRT$(11,1)=RPT_AUX1$; PRT$(12,1)=RPT_AUX2$
20140 PRT$(13,1)=RPT_AUX3$; PRT$(14,1)=RPT_AUX4$
20150 PRT$(15,2)=RPT_LINES$; PRT$(17,1)=RPT_FORMCODE$
20160 PRT$(65,60)=RPT_JOBCARD$; PRT$(125,1)=RPT_JOBCARD_FLG$
20170 ! 
20175 RNG$=""
20180 ! 
20190 IF RPT_SEQ$="1" THEN {
20200 T1$="FM1..."
20210 GOSUB GET_NOTE_TYPE_RANGE; GOSUB GET_NOTE_FMT_RANGE; GOSUB GET_CUST_RANGE; GOSUB GET_LOC_RANGE; GOSUB GET_ITEM_RANGE }
20380 IF B_ZZS$=ZZS$ AND B_PRT$=PRT$ AND B_RNG$=RNG$ THEN CHG_FLG=0 ELSE CHG_FLG=1
20390 RETURN 
25000 ! 
25010 PRINT_REPORT:
25020 IF NOT(NUL(VIEW$)) THEN PRT$(2,2)=VIEW$
25030 K$=X3$(1,8)
25040 %B_WINDATE_FMT$=%WINDATE_FMT$,%WINDATE_FMT$="YYYYMMDD",JJ$=DTE(0:"%Yl%Mz%Dz"); CALL "ZZWDTE;VALIDATE",JJ$,JJJ$,JJJJ$; X3$(21,6)=JJ$
25050 WRITE (Z[7],KEY=K$)X3$,X4$,T1$,PRT$,RNG$,ZZS$
25053 K9$="FM2RFA"
25060 CALL K9$
25070 CMD_STR$="END"
25080 RETURN 
28000 ! 
28010 GET_CUST_RANGE:
28020 IF NUL(CUST_START$) THEN RNG$+="          " ELSE RNG$+=CUST_START$
28030 IF NUL(CUST_END$) THEN RNG$=RNG$+"~~~~~~~~~~" ELSE RNG$=RNG$+CUST_END$ }
28040 RETURN 
28050 ! 
28060 GET_ITEM_RANGE:
28070 IF NUL(ITEM_START$) THEN RNG$+="          " ELSE RNG$+=PAD(ITEM_START$,10)
28080 IF NUL(ITEM_END$) THEN RNG$+="~~~~~~~~~~" ELSE RNG$+=PAD(ITEM_END$,10)
28090 RETURN 
28100 ! 
28110 GET_LOC_RANGE:
28120 IF NUL(LOC_START$) THEN RNG$+="    " ELSE RNG$=RNG$+LOC_START$
28130 IF NUL(LOC_END$) THEN RNG$=RNG$+"~~~~" ELSE RNG$=RNG$+LOC_END$
28140 RETURN 
28150 ! 
28160 GET_NOTE_TYPE_RANGE:
28170 IF NUL(NOT_TYPE_START$) THEN RNG$+=" " ELSE RNG$+=NOT_TYPE_START$
28180 IF NUL(NOT_TYPE_END$) THEN RNG$+="~" ELSE RNG$+=NOT_TYPE_END$
28190 RETURN 
28200 ! 
28210 GET_NOTE_FMT_RANGE:
28220 IF NUL(NOT_FORMAT_START$) THEN RNG$+=" " ELSE RNG$+=NOT_FORMAT_START$
28230 IF NUL(NOT_FORMAT_END$) THEN RNG$+="~" ELSE RNG$+=NOT_FORMAT_END$
28240 RETURN 
28250 ! 
30000 SELECT_SELECTION:
30010 _KEY$="FM2RFA"+RPT_SELEC$,_KEY$=PAD(_KEY$,18," ")
30020 READ (Z(1),KEY=_KEY$,DOM=NEW_RECORD1)A$,R0$,W$,RNG{ALL}
30030 GOTO MAIN_POST_DISPLAY
30500 NEW_RECORD1:
30510 MSGBOX MSG("ADD_RECORD"),MSG("CONFIRM"),"?,YESNO",_YESNO$
30520 IF _YESNO$="NO" THEN GOSUB MAIN_POST_DISPLAY; EXIT 
30545 ZZS$(71,1)="1",ZZS$(1,6)="FM2RFA",ZZS$(7,LEN(RPT_SELEC$))=RPT_SELEC$
30546 ZZS$(72,1)="N",ZZS$(73,6)="",ZZS$(79,1)="N",ZZS$(111,1)="N",ZZS$(112,1)="N"
30550 WRITE (Z[1],KEY=_KEY$)IOL=0170,IOL=0180
30560 GOTO MAIN_POST_DISPLAY

28150 ! 
28160 GET_PROD_RANGE:
28170 IF NUL(PROD_START$) THEN RNG$=RNG$+"   " ELSE RNG$=RNG$+PROD_START$
28180 IF NUL(PROD_END$) THEN RNG$=RNG$+"~~~" ELSE RNG$=RNG$+PROD_END$

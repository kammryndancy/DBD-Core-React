1086 IF RNG$(37,3)="~~~" THEN ORD_END$="" ELSE ORD_END$=RNG$(37,8)
1090 NEXTFOLDER_FLG=FLDR.FM1.7.3.CTL
1095 RETURN 
1100 CASE "2"

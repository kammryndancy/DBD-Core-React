1170 IF NUL(RNG$(31,10)) THEN ITEM_START$="" ELSE ITEM_START$=RNG$(31,10)
1180 IF RNG$(41,3)="~~~" THEN ITEM_END$="" ELSE ITEM_END$=RNG$(41,10)
1190 NEXTFOLDER_FLG=FLDR.FM1.7.1.CTL
1200 RETURN 
1300 CASE "3"
1310 IF NUL(RNG$(1,3)) THEN PROD_START$="" ELSE PROD_START$=RNG$(1,3)
1320 IF RNG$(4,3)="~~~" THEN PROD_END$="" ELSE PROD_END$=RNG$(4,3)
1380 NEXTFOLDER_FLG=FLDR.FM1.7.4.CTL
1390 RETURN 
1490 END SWITCH 
1500 RETURN 
3800 CHECK_CHANGES:
3805 GOSUB LOAD_OPTIONS

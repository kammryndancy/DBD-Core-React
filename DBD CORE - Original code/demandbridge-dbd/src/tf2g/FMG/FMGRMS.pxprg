20065 IF NUL(END_DATE$) THEN ZZS$(78,6)="~~~~~~" ELSE ZZS$(78,6)=END_DATE$
20070 ZZS$(86,1)=INV_DATE$; ZZS$(85,1)=INV_INFO$; ZZS$(87,1)=OMIT_COST$
20080 ZZS$(89,1)=SHIP_DET$; ZZS$(90,1)=B_TYPE$; ZZS$(88,1)=REP_TOT$

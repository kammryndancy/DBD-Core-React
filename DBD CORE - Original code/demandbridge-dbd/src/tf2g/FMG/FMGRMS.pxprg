1070 IF NUL(RNG$(9,10)) THEN CUST_START$="" ELSE CUST_START$=RNG$(9,10)
1080 IF RNG$(19,3)="~~~" THEN CUST_END$="" ELSE CUST_END$=RNG$(19,10)

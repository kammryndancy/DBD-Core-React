28250 ! 
28260 GET_ORD_RANGE:
28270 IF NUL(ORD_START$) THEN RNG$=RNG$+"        " ELSE CALL "ZZWLKU;PARSE_ORD",ORD_START$,ORD_DIV$,ORD_NUM$; ORD_START$=ORD_DIV$+ORD_NUM$,RNG$=RNG$+ORD_START$
28280 IF NUL(ORD_END$) THEN RNG$=RNG$+"~~~~~~~~" ELSE CALL "ZZWLKU;PARSE_ORD",ORD_END$,ORD_DIV$,ORD_NUM$; ORD_END$=ORD_DIV$+ORD_NUM$,RNG$=RNG$+ORD_END$
28290 RETURN 

25060 K9$="FM2RMA" ! SSP 229934
25065 IF RB_PRT$>"0" THEN CALL "ZZ2DRV",K9$ ELSE CALL K9$ ! SSP 229934
25070 CMD_STR$="END"
25080 RETURN 
28000 ! 
28010 GET_CUST_RANGE:
28020 IF NUL(CUST_START$) THEN RNG$+=DIM(10) ELSE RNG$+=CUST_START$
28030 IF NUL(CUST_END$) THEN RNG$+=DIM(10,"~") ELSE RNG$=RNG$+CUST_END$
28040 RETURN 
28050 ! 
28060 GET_SLSMAN_RANGE:
28070 IF NUL(SLS_START$) THEN RNG$+=DIM(4) ELSE RNG$+=SLS_START$
28080 IF NUL(SLS_END$) THEN RNG$+=DIM(4,"~") ELSE RNG$+=SLS_END$
28090 RETURN 
28100 ! 
28110 GET_ITEM_RANGE:

13000 ! 13000 - Clear the record fields
13010 CLEAR_FIELDS:
13020 IF _CLR_FLG$="R" THEN ML_TYPE$=""
13030 ML_DESC$="",NUM_OF_LINE=0,FIRST_LINE=0,WIDTH=0,NUM_LN_SECTION=0
13040 ACCT_TYPE$=""
13050 SUB_LEDGER$=""
13060 COST_CODE_TYPE$=""
13070 GL1_UNUSED_1$=""
13980 GOSUB NUM_TO_STR

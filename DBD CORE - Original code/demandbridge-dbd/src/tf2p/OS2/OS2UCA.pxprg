4067 IF OS5$(38,1)<>SPFX$ THEN GOTO 4061; REM " PRICE NOT LOADED FROM TAPE
4068 IF OS5$(37,1)="C" THEN GOTO 4061; REM " CONTRACT PRICE
4069 REM "IF OS5$(39,1)<>"L" THEN GOTO 4061; REM "MANUALLY ENTERED PRICE
4070 OS5_ARRAY$=OS5_ARRAY$+OS5$(1,6)+OS5$(37,1)+STR(OS5[0]:M0$)+OS5$(40,12)+OS5$(52,1)+OS5$(53,6)+STR(OS5[1]:M0$)

1000 REM "
1010 GOSUB 4000; REM " LOAD THE OS5 ARRAY
1015 X_MAX=LEN(OS5_ARRAY$)/OS5_CHUNK-1 ! ssp# 198019
1020 IF X_MAX<0 THEN GOTO 5000 ! ssp# 198019
1040 OS0_KEY$=KEY(Z[2],END=5000)
1050 READ (Z[2],KEY=OS0_KEY$)IOL=0320
1051 C=C+1
1055 FOR X=0 TO X_MAX ! ssp# 198019
1056 OS5$=OS5_ARRAY$(X*OS5_CHUNK+1,OS5_CHUNK) ! ssp# 198019
1075 DIM OS1$(250); READ (Z[3],KEY=OS0$(1,10)+OS5$(1,6),DOM=1076)IOL=0321; GOTO 1200
1080 OS1$(1,10)=OS0$(1,10),OS1$(11,6)=OS5$(1,6),OS1$(25,12)=OS5$(18,12)
1200 REM " FOUND THE RECORD SO UPDATE ONLY PRICE
1201 REM "IF OS1$(17,1)<>"L" THEN GOTO 2000
1203 OS1$(25,12)=OS5$(18,12); REM SSP 204616
1205 IF OS5$(31,6)<>DIM(6) THEN GOSUB 3000 ELSE GOSUB 3500
1210 BASE_PRICE=NUM(OS0$(204,10))
1215 RATE=NUM(OS5$(8,10))/100
1216 FLOOR=NUM(OS5$(37,10))/100
1220 IF OS5$(7,1)="M" THEN ADJUSTMENT=COST*RATE; PRICE=COST+ADJUSTMENT
1221 IF OS5$(7,1)="M" AND PRICE>BASE_PRICE THEN PRICE=BASE_PRICE; REM SSP 199949
1225 IF OS5$(7,1)="D" THEN ADJUSTMENT=BASE_PRICE*RATE; PRICE=BASE_PRICE-ADJUSTMENT
1226 IF FLOOR=0 THEN GOTO 1234
1227 IF PRICE=0 THEN GOTO 1234
1228 GP=1-(COST/PRICE); IF GP<FLOOR THEN PRICE=COST*100/(100-FLOOR*100)
1233 IF PRICE>BASE_PRICE THEN PRICE=BASE_PRICE; REM SSP
1234 IF OS5$(7,1)="G" THEN PRICE=COST*100/(100-RATE*100)
1235 OS1$(57,10)=STR(COST:"#######.00")
1237 IF OS5$(7,1)="L" THEN PRICE=BASE_PRICE
1240 OS1$(47,10)=STR(PRICE:"#######.00")
1241 OS1$(237,10)=OS0$(204,10)
1245 OS1$(17,1)="C"; REM " CALCULATED PRICE
1246 READ (Z[6],KEY=OS1$(11,6),DOM=*NEXT)OS5_SECOND$; REM SSP 197544
1247 OS1$(18,1)=OS5_SECOND$(59,1); REM SSP 197544
1250 IF OSP$(459,1)="N" AND NUM(OS1$(47,10))<>0 AND NUM(OS1$(57,10))<>0 THEN WRITE (Z[3],KEY=OS1$(1,16))IOL=0321 ELSE IF OSP$(459,1)="Y" THEN WRITE (Z[3],KEY=OS1$(1,16))IOL=0321
1251 BASE_PRICE=0,COST=0,RATE1=0,RATE2=0,RATE3=0,RATE4=0,PRICE1=0,PRICE2=0,PRICE3=0,PRICE4=0
1260 NEXT X
1500 IF MOD(C,T0)=1 THEN GOSUB 8150
2000 GOTO 1040
3000 REM "FIGURE OUT THE COST TO USE
3001 COST=0
3005 DIM OS7$(125); READ (Z[5],KEY=OS0$(1,10)+OS5$(31,6),DOM=3199)IOL=0325
3007 IF OS7$=DIM(125) THEN GOTO 3199
3008 IF OS5$(30,1)=" " THEN OS5$(30,1)="3"
3010 IF OS5$(30,1)="3" THEN COST=NUM(OS7$(67,10))
3020 IF OS5$(30,1)="2" THEN COST=NUM(OS7$(47,10))
3030 IF OS5$(30,1)="1" THEN COST=NUM(OS7$(27,10))
3040 IF COST<>0 THEN GOTO 3199
3050 IF OS5$(30,1)="3" THEN OS5$(30,1)="2" ELSE IF OS5$(30,1)="2" THEN OS5$(30,1)="1"
3060 GOTO 3007
3199 RETURN 
3500 REM " GET LOWEST COST
3525 READ (Z[5],KEY=OS0$(1,10),DOM=*NEXT)
3530 OS7_KEY$=KEY(Z[5],END=3599)
3535 IF OS7_KEY$(1,10)<>OS0$(1,10) THEN GOTO 3599
3540 READ (Z[5],KEY=OS7_KEY$)OS7$
3545 IF OSP$(225,1)="1" THEN COST$=OS7$(27,10) ELSE IF OSP$(225,1)="2" THEN COST$=OS7$(47,10) ELSE IF OSP$(225,1)="3" THEN COST$=OS7$(67,10)
3550 IF COST=0 THEN COST=NUM(COST$)
3555 IF NUM(COST$)<COST THEN COST=NUM(COST$)
3565 GOTO 3530
3599 RETURN 
4000 REM "LOAD THE OS5 ARRAY - FOR EACH CONSUMER PRICE PLAN

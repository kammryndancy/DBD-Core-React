10000 ADD_FS6_LINE_NUM:
10002 ENTER X3$,X4$,OPTIONS$,ERRMSG$,ERR=*NEXT
10005 IOLIST FS6L$,FS6L[0],FS6L[1],FS6L[2],FS6L[3],FS6L[4],FS6L[5],FS6L[6],FS6L[7]
10015 DIM FS6L$(163),FS6L[7]
10020 OPEN (HFN)"FS6"+%C$; _FS6L=LFO
10030 K$=KEL(_FS6L,END=EXIT_ADD); READ (_FS6L,KEY=K$)IOL=10005
10040 IF MID(STP(FS6L$(114,3)),2)="" THEN GOTO ADD_LINES ELSE GOTO EXIT_ADD
10050 ADD_LINES:
10060 READ (_FS6L,KEY="",DOM=READ_NEXT)
10070 READ_NEXT:
10080 K$=KEY(_FS6L,END=EXIT_ADD); READ (_FS6L,KEY=K$)IOL=10005
10090 LN$=STR(ASC(K$(30,1))-48:"000")
10100 IF NUM(LN$)>=100 THEN LN$=STR(NUM(LN$)-10)
10110 FS6L$(114,3)=LN$,FS6L$(117,3)="001"
10120 WRITE (_FS6L,KEY=K$)IOL=10005
10130 ! RCNT=RCNT+1; IF MOD(RCNT,1000)=0 THEN PRINT @(10,10),STR(RCNT)," ", K$
10140 GOTO READ_NEXT
10160 EXIT_ADD:
10170 MSGBOX "Process Completed !!","F.Y.I","!,TIM=3)"
10185 CLOSE (_FS6L)
10195 EXIT 

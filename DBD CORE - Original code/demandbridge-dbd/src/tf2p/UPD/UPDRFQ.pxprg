2140 SEQ=SEQ+1; IF SEQ>999999 THEN GOTO 2190
2150 CALL "UPDRFQ;READBYKEY",RFQ,0,RFQ$(1,12),STR(SEQ:"000000"),SEG3$,SEG4$,SEG5$,RFQ_TMP$,RFQ_TMP{ALL},FOUNDRFQ; IF FOUNDRFQ THEN GOTO NEXT_SEQ
2160 RFQ$(13,6)=STR(SEQ:"000000")

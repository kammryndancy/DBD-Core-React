10000 ADD_TO_QUEUE:
10010 ENTER FILENO,MEM_FIL,OUTPUT_TYPE$
10011 S_TRANS_NUM$=""
10012 READ (MEM_FIL,KEY="",ERR=*NEXT)
10015 READ (MEM_FIL,END=10090)MEM_K$,TRANS_NUM$,TOD_SEQ,TOD_OUTPUT_TYPE$; IF POS(TRANS_NUM$=S_TRANS_NUM$,10)=0 THEN S_TRANS_NUM$=S_TRANS_NUM$+TRANS_NUM$ ELSE GOTO *SAME
10020 IF TOD_OUTPUT_TYPE$<>"" THEN OUTPUT_TYPE$=TOD_OUTPUT_TYPE$ ! used if multiple types written to mem file 
10050 CALL "UPDDT6;CLEAR_FIELDS",DT6$,DT6{ALL}
10060 DT6$(1,10)=TRANS_NUM$,DT6$(11,20)=OUTPUT_TYPE$
10070 CALL "UPDDT6;UPDATE",FILENO,DT6$,DT6{ALL},BUSY
10080 GOTO 10015
10090 EXIT 

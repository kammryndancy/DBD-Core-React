0010 REM "A/P Invoice data entry setups <AP2DAC>
0020 SETESC 9300; SETERR 9000
0040 REM "Copyright 1986 Computer Software Inc.; Norcross, Georgia.
0041 REM "        Licensed Software - All Rights Reserved.
0100 SETERR 9000
0200 ON J9 GOTO 9900,0900,1100,1200,1300
0900 REM 
0920 PRINT @(0,3),'CE',
0925 Q1$=""
1000 CALL "AP2MAB",X3$,X4$,Q0$,Q1$
1010 IF Q1$<>"" THEN A$(7,10)=Q1$,C0=1 ELSE C0=0
1090 GOTO 9900
1100 REM "Terms code
1110 Q1$=""
1120 CALL "AP2MPE",X3$,X4$,Q0$,Q1$
1130 IF Q1$<>"" THEN A$(33,2)=Q1$,C0=1 ELSE C0=0
1140 GOTO 9900
1200 REM "Apply to invoice
1210 Q1$=A$(7,10)
1220 CALL "AP2API",X3$,X4$,Q0$,Q1$
1230 IF LEN(Q1$)>10 THEN A$(90,10)=Q1$(11),C0=1 ELSE C0=0
1240 GOTO 9900
1300 REM "G/L category
1310 Q1$=""
1320 CALL "AP2MPF",X3$,X4$,Q0$,Q1$
1330 IF Q1$<>"" THEN A$(75,9)=Q1$,C0=1 ELSE C0=0
1340 GOTO 9900
9000 REM "ERROR PROCESSING
9005 IF ERR=69 THEN GOTO 9500
9010 Y5=ERR,Y6=TCB(5)
9015 SETERR 9016; Y8$=LST(PGM(Y6))
9016 SETERR 9000
9040 CALL "ZZERRM",Y8$,X0$,Y7$,X3$,Y5,Y6,Y7,Y8,0
9050 ON Y7 GOTO 9060,9100,9900,9070,9090
9060 RETRY 
9070 SETERR 9080
9075 EXECUTE Y7$
9080 SETERR 9000; RETRY 
9090 SETERR 0000; RETRY 
9100 REM " TRANSFER CONTROL
9180 GOTO 0990
9190 GOTO 9900
9300 SETESC 9350
9310 SETERR 9350
9315 IF X3$(47,1)="N" THEN RETURN ELSE SETESC 0000; RETURN 
9350 SETERR 9000; RETURN 
9500 REM "CTRL LOGIC
9510 SETERR 9000; GOSUB 6400
9520 ON C9 GOTO 1140,2040
9800 REM "EXIT PROGRAM
9900 REM "END PROGRAM
9950 RUN "AP2DAD"
9999 END 

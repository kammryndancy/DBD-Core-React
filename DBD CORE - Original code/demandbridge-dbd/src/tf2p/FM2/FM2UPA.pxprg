3325 IF K0$(1,20)<>A$(1,20) THEN GOTO 3390
3330 IF K0$(25,4)<=Y4$ THEN REMOVE (Z[3],KEY=K0$); IF RT_PARM$<>"" THEN CUST$=K0$(1,10); CALL "RT2WOC",ERR=3331,X3$,X4$,CUST$,"FM3...","D",K0$
3335 GOTO 3320
3390 RETURN 
